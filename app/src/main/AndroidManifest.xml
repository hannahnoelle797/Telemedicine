<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.telemedicine">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.scheduling.ApptSchedulingFirebase"></activity>
        <activity
            android:name=".ui.signup.Signup"
            android:label="Sign up" />
        <activity
            android:name=".ui.login.Login"
            android:label="@string/Login"></activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.sample_pages.sample_report"></activity>
        <activity android:name=".ui.sample_pages.sample_appt"></activity>
        <activity android:name=".ui.sample_pages.sample_chat"></activity>
    </application>

</manifest>